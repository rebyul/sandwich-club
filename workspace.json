{
  "version": 2,
  "projects": {
    "account": {
      "root": "libs/account",
      "sourceRoot": "libs/account/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/account/**/*.{ts,tsx,js,jsx}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/account"
          ],
          "options": {
            "jestConfig": "libs/account/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": [
              "apps/api/src/assets"
            ],
            "generatePackageJson": true,
            "target": "node",
            "compiler": "tsc"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/api/src/environments/environment.ts",
                  "with": "apps/api/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:node",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "deploy": {
          "executor": "nx:run-commands",
          "options": {
            "commands": [
              "docker build -f ./apps/api/Dockerfile . -t sandwich-club/api"
            ],
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/api/**/*.ts"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/api"
          ],
          "options": {
            "jestConfig": "apps/api/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "auth": {
      "root": "libs/auth",
      "sourceRoot": "libs/auth/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/auth/**/*.{ts,tsx,js,jsx}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/auth"
          ],
          "options": {
            "jestConfig": "libs/auth/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "finances": {
      "root": "libs/finances",
      "sourceRoot": "libs/finances/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/finances/**/*.ts"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/finances"
          ],
          "options": {
            "jestConfig": "libs/finances/jest.config.ts",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "membership": {
      "root": "libs/membership",
      "sourceRoot": "libs/membership/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "lint": {
            "executor": "@nrwl/linter:eslint",
            "options": {
              "lintFilePatterns": [
                "apps/sandwich-club-e2e/**/*.{js,ts}"
              ]
            }
          },
          "test": {
            "executor": "@nrwl/jest:jest",
            "outputs": [
              "coverage/libs/membership"
            ],
            "options": {
              "jestConfig": "libs/membership/jest.config.js",
              "passWithNoTests": true
            }
          }
        }
      },
      "tags": []
    },
    "sandwich-club": {
      "root": "apps/sandwich-club",
      "sourceRoot": "apps/sandwich-club/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/webpack:webpack",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/sandwich-club",
            "index": "apps/sandwich-club/src/index.html",
            "main": "apps/sandwich-club/src/main.tsx",
            "polyfills": "apps/sandwich-club/src/polyfills.ts",
            "tsConfig": "apps/sandwich-club/tsconfig.app.json",
            "assets": [
              "apps/sandwich-club/src/favicon.ico",
              "apps/sandwich-club/src/assets"
            ],
            "generatePackageJson": true,
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/sandwich-club/src/environments/environment.ts",
                  "with": "apps/sandwich-club/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "deploy": {
          "executor": "nx:run-commands",
          "options": {
            "commands": [
              "docker build -f ./apps/sandwich-club/Dockerfile . -t sandwichclub/react"
            ],
            "parallel": false
          }
        },
        "serve": {
          "executor": "@nrwl/webpack:dev-server",
          "options": {
            "buildTarget": "sandwich-club:build",
            "proxyConfig": "apps/sandwich-club/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "sandwich-club:build:production"
            },
            "development": {
              "buildTarget": "sandwich-club:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sandwich-club/**/*.{ts,tsx,js,jsx}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/sandwich-club"
          ],
          "options": {
            "jestConfig": "apps/sandwich-club/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "container": {
          "executor": "@nx-tools/nx-container:build",
          "options": {
            "engine": "docker",
            "metadata": {
              "images": [
                "sandwich-club/undefined"
              ],
              "load": true,
              "tags": [
                "type=schedule",
                "type=ref,event=branch",
                "type=ref,event=tag",
                "type=ref,event=pr",
                "type=sha,prefix=sha-"
              ]
            }
          }
        }
      },
      "tags": []
    },
    "sandwich-club-e2e": {
      "root": "apps/sandwich-club-e2e",
      "sourceRoot": "apps/sandwich-club-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/sandwich-club-e2e/cypress.json",
            "tsConfig": "apps/sandwich-club-e2e/tsconfig.e2e.json",
            "devServerTarget": "sandwich-club:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "sandwich-club:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/sandwich-club-e2e/**/*.{js,ts}"
            ]
          },
          "outputs": [
            "{options.outputFile}"
          ]
        }
      },
      "tags": [],
      "implicitDependencies": [
        "sandwich-club"
      ]
    }
  }
}
